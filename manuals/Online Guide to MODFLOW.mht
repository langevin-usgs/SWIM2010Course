function HMToggle()
{=20
	var op =3D HMToggle.arguments[0];
	for (i=3D1; i<HMToggle.arguments.length; i++) {
	  var objID =3D HMToggle.arguments[i];=20
       	  var obj =3D document.getElementById(objID);
 	  switch (op) {
		case "toggle":=20
		  var state =3D obj.getAttribute("hm.state");
		  if (state =3D=3D null) { state =3D "0" };=20
		  HMToggleExpand(obj, (state !=3D "1"));
		  break;

		case "expand":
		  HMToggleExpand(obj, true);
		  break;

		case "collapse":
		  HMToggleExpand(obj, false);
		  break;
	  }
	}
}

function HMToggleExpand(obj, value)
{
	tagName =3D obj.nodeName.toLowerCase();
	switch (tagName) {
		case "span":
		  obj.style.display =3D (value ? "inline" : "none");
		  break;
		case "table":
