obj.getAttribute("hm.src0"));
		  var newTitle =3D (value ? obj.getAttribute("hm.title1") : =
obj.getAttribute("hm.title0"));
		  if (newTitle !=3D null) { obj.title =3D newTitle; }
		  var newCaption =3D (value ? obj.getAttribute("hm.caption1") : =
obj.getAttribute("hm.caption0"));
		  if (newCaption !=3D null) { =
obj.parentNode.parentNode.parentNode.nextSibling.firstChild.firstChild.in=
nerHTML =3D newCaption; }
		  break;
	}
	obj.setAttribute("hm.state", value ? "1" : "0");
}

function HMInitToggle()
{
    if (document.getElementById) {
	var node =3D document.getElementById(HMInitToggle.arguments[0]);
	for (i=3D1; i<HMInitToggle.arguments.length-1; i=3Di+2) {=20
		if (HMInitToggle.arguments[i] =3D=3D "onclick") {
		  node.onclick =3D Function(HMInitToggle.arguments[i+1]);=20
		}
		else {=20
		  node.setAttribute(HMInitToggle.arguments[i], =
decodeURI(HMInitToggle.arguments[i+1]));=20
		}
		if (HMInitToggle.arguments[i].substring(0,6) =3D=3D "hm.src") {
			var img =3D new Image();
			img.src =3D HMInitToggle.arguments[i+1];
		}
	}
	mustExpand =3D window.location.search.lastIndexOf("zoom_highlight") > =
0;
	if (node.nodeName.toLowerCase() =3D=3D "img") {
	   var aLink =3D node.parentNode;
	   if (aLink.nodeName.toLowerCase() =3D=3D "a") {=20
		aLink.href =3D "javascript:HMToggle('toggle','" + =
HMInitToggle.arguments[0] +"')";
		mustExpand =3D false;=20
	   }
	}
	HMToggles[HMToggles.length] =3D node;
	HMToggleExpand(node, ((node.getAttribute("hm.state") =3D=3D "1") || =
