function findPosY(obj)
{
    var curtop =3D 0;
    if (obj.offsetParent)
    {
        while (obj.offsetParent)
        {
            curtop +=3D obj.offsetTop
            obj =3D obj.offsetParent;
        }
    }
    else if (obj.y)
        curtop +=3D obj.y;
    return curtop;
}


// regular expression version
function SearchHiLite(text)
{
    var SearchAsSubstring =3D 0;
    var hl;

    hl =3D QueryString("zoom_highlight");
    if (hl =3D=3D "" || hl =3D=3D null)
    {
        hl =3D QueryString("zoom_highlightsub");
        if (hl =3D=3D "" || hl =3D=3D null)
            return false;
        else
            SearchAsSubstring =3D 1;
    }
    if ((document.charset && document.charset =3D=3D "utf-8") ||
    	(document.characterSet && document.characterSet =3D=3D "UTF-8"))
    	hl =3D decodeURIComponent(hl);
    else
    	hl =3D unescape(hl);
    hl =3D hl.toLowerCase();
               =20
    // create array of terms       =20
    //var term =3D hl.split("+");=20
    var re =3D /\"(.*?)\"|[^\\+\"]+/g;
    var term =3D hl.match(re);   =20
  =20
    // convert terms in regexp patterns
    for (var i=3D0;i<term.length;i++) // take each term in turn
    {      =20
        if(term[i] !=3D "")
        {                  =20
            if (term[i].indexOf("\"") !=3D -1)
            {
                // contains double quotes              =20
                term[i]=3Dterm[i].replace(/\"/g,"");
                term[i]=3Dterm[i].replace(/\+/g," ");=20
            }
            else
            {
                term[i]=3Dterm[i].replace(/\+/g,""); =20
            }                          =20

            if (term[i].indexOf("*") !=3D -1 || term[i].indexOf("?") =
!=3D -1)
            {
                // convert wildcard pattern to regexp
                term[i] =3D term[i].replace(/\\/g, " ");
                term[i] =3D term[i].replace(/\^/g, " ");

                //term[i] =3D term[i].replace(/\+/g, " "); // split on =
this so no point in looking

                term[i] =3D term[i].replace(/\#/g, " ");
                term[i] =3D term[i].replace(/\$/g, " ");
                term[i] =3D term[i].replace(/\./g, " ");
               =20
                // check if search term only contains only wildcards
                // if so, we will not attempt to highlight this term
                var wildcards =3D /\w/;
                if (wildcards.test(term[i]))
                {
                	term[i] =3D term[i].replace(/\*/g, "[^\\s=07]*");
                	term[i] =3D term[i].replace(/\?/g, "[^\\s=07]"); // =
insist upon one non whitespace
                }               =20
                else               =20
                	term[i] =3D "";               =20
            }
		=09
			if (term[i] !=3D "")
			{
	            if (SearchAsSubstring =3D=3D 0)
	            {	               =20
	                term[i] =3D =
"(>[\\s]*|>[^<=07]+[\\b\\W])("+term[i]+")(<|[\\b\\W][^>]*<)";
	            }
	            else
	            {
	                // if term leads with wildcard then allow it to match =
preceeding text in word
	                var strWB=3D"";
	                if(term[i].substr(0,7)=3D=3D"[^\\s=07]*") =
strWB=3D"\\b";
	                term[i] =3D =
"(>|>[^<=07]+)"+strWB+"("+term[i]+")([^>]*<)";
	            }
	        }	       =20
        }
    }

    text=3Dtext.replace(/&amp;/ig, '&');
    text=3Dtext.replace(/&nbsp;/ig, '=08');

    for (var i=3D0;i<term.length;i++) // take each term in turn
    {
        if(term[i] !=3D "")
        {        	        =09
            // we need a loop for the main search to catch all between =
><
            // and we add =07 before each found to ignore those done etc
            // todo: develop reliable single pass regexp and dispose of =
loop
            var l =3D 0;
            re =3D new RegExp(term[i], "gi");
            var count =3D 0; // just incase
            text =3D ">" + text + "<"; // temporary tag marks
            do=20
            {
                l=3Dtext.length;
                text=3Dtext.replace(re, '$1<span =
style=3D"background:#FFFF40;" class=3D"highlight" id=3D"highlight" =
name=3D"highlight">=07$2</span id=3D"highlight">$3');
                count++;
            }
            //while(re.lastIndex>0 && count<100); lastIndex not set =
properly under netscape
            while(l!=3Dtext.length && count<100);
            text =3D text.substring(1, text.length-1); // remove =
temporary tags
        }
    }       =20
    text =3D text.replace(eval("/=07/g"), '');       =20
    text =3D text.replace(eval("/=08/g"), '&nbsp;');   =20
      =20
    return(text);
}

function jumpHL()
{
